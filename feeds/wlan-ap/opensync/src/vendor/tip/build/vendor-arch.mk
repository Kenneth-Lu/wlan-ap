OS_TARGETS +=TIP
OS_TARGETS +=USE_NM2

TARGET ?= $(DEFAULT_TARGET)

ifneq ($(filter TIP USE_NM2,$(TARGET)),)
PLATFORM=openwrt
VENDOR=tip
PLATFORM_DIR := platform/$(PLATFORM)
VENDOR_DIR := vendor/$(VENDOR)
KCONFIG_TARGET ?= $(VENDOR_DIR)/kconfig/targets/$(TARGET)
ARCH_MK := $(PLATFORM_DIR)/build/$(PLATFORM).mk
endif
